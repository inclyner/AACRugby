<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="515.0" prefWidth="725.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.manager.ManagerMainController">
   <children>
      <Button fx:id="btnInsertUser" layoutX="50.0" layoutY="54.0" mnemonicParsing="false" onMouseClicked="#onClickInsertUser" prefHeight="34.0" prefWidth="152.0" style="-fx-background-color: rgba(193,166,112,255);" text="InsertUser">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button fx:id="btnDeleteUser" layoutX="50.0" layoutY="95.0" mnemonicParsing="false" prefHeight="34.0" prefWidth="152.0" style="-fx-background-color: rgba(193,166,112,255);" text="Delete User">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button fx:id="btnAproveRequests" layoutX="50.0" layoutY="139.0" mnemonicParsing="false" prefHeight="34.0" prefWidth="152.0" style="-fx-background-color: rgba(193,166,112,255);" text="Approval Request">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <ImageView fx:id="accountIcon" fitHeight="265.0" fitWidth="320.0" layoutX="441.0" layoutY="41.0" onMouseClicked="#onClickAccountIcon" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@account_icon.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
   </children>
</AnchorPane>
