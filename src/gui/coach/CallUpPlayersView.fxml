<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="476.0" prefWidth="608.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.coach.CallUpPlayersController">
   <children>
      <Label layoutX="47.0" layoutY="39.0" text="Call Up Players for next game">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <TableView fx:id="tableViewCallUpPlayers" layoutX="67.0" layoutY="138.0" prefHeight="200.0" prefWidth="473.0">
        <columns>
          <TableColumn fx:id="name" prefWidth="334.0" text="Name" />
          <TableColumn fx:id="callToGame" prefWidth="138.0" text="Call to Game" />
        </columns>
      </TableView>
      <Button fx:id="btnBack" layoutX="58.0" layoutY="408.0" mnemonicParsing="false" onAction="#onClickBtnBack" prefHeight="30.0" prefWidth="58.0" style="-fx-background-color: rgba(193,166,112,255);" text="Back">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="btnSave" layoutX="497.0" layoutY="408.0" mnemonicParsing="false" onAction="#onClickBtnSave" prefHeight="30.0" prefWidth="58.0" style="-fx-background-color: rgba(193,166,112,255);" text="Save">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Label layoutX="68.0" layoutY="109.0" text="Players: ">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <CheckBox fx:id="chbSelectAllPlayers" layoutX="61.0" layoutY="362.0" mnemonicParsing="false" onAction="#onSelectchbAllPlayers" text="Select all Players">
         <font>
            <Font size="13.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </CheckBox>
   </children>
</AnchorPane>
