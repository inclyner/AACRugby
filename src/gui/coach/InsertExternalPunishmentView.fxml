<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="476.0" prefWidth="607.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.coach.InsertExternalPunishmentController">
   <children>
      <Label layoutX="48.0" layoutY="44.0" text="Insert External Punishment for Player">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label layoutX="55.0" layoutY="110.0" text="Player">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <ComboBox fx:id="cmbPlayer" layoutX="110.0" layoutY="105.0" prefWidth="150.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ComboBox>
      <Spinner fx:id="tfNumberGames" editable="true" layoutX="461.0" layoutY="106.0" prefHeight="26.0" prefWidth="69.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Spinner>
      <Label layoutX="346.0" layoutY="110.0" text="Number of Games: ">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <TextArea fx:id="tfPunishmentNote" layoutX="55.0" layoutY="231.0" prefHeight="130.0" prefWidth="477.0">
         <cursor>
            <Cursor fx:constant="TEXT" />
         </cursor>
      </TextArea>
      <Label layoutX="55.0" layoutY="197.0" text="Punishment Notes:" />
      <Button fx:id="btnBack" layoutX="41.0" layoutY="410.0" mnemonicParsing="false" onAction="#onClickBtnBack" prefHeight="37.0" prefWidth="62.0" style="-fx-background-color: rgba(193,166,112,255);" text="Back">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="btnSave" layoutX="496.0" layoutY="410.0" mnemonicParsing="false" onAction="#onClickBtnSave" prefHeight="38.0" prefWidth="62.0" style="-fx-background-color: rgba(193,166,112,255);" text="Save">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
